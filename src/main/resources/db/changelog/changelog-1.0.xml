<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
	xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
	xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd
                        http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd">

	<!-- JHipster core tables. The initial schema has the '00000000000001' id, 
		so that it is over-written if we re-generate it. -->
	<changeSet id="00000000000000" author="diego.aires">
		<createTable tableName="uf">
			<column name="id" type="bigint">
				<constraints primaryKey="true" nullable="false" />
			</column>
			<column name="nome" type="varchar(255)" />
			<column name="sigla" type="varchar(2)" />
		</createTable>
		<insert tableName="uf">
			<column name="id" value="1" />
			<column name="nome" value="Tocantins" />
			<column name="sigla" value="TO" />
		</insert>
		<insert tableName="uf">
			<column name="id" value="2" />
			<column name="nome" value="BrasÃ­lia" />
			<column name="sigla" value="DF" />
		</insert>

		<createTable tableName="tipoendereco">
			<column name="codtipoendereco" type="smallint" />
			<column name="desctipoendereco" type="varchar(255)" />
		</createTable>
		<insert tableName="tipoendereco">
			<column name="codtipoendereco" value="1" />
			<column name="desctipoendereco" value="RESIDENCIAL" />
		</insert>
		<insert tableName="tipoendereco">
			<column name="codtipoendereco" value="2" />
			<column name="desctipoendereco" value="COMERCIAL" />
		</insert>
		<insert tableName="tipoendereco">
			<column name="codtipoendereco" value="3" />
