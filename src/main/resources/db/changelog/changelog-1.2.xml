<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.6.xsd">

    <!--
        JHipster core tables.
        The initial schema has the '00000000000001' id, so that it is over-written if we re-generate it.
    -->
    <changeSet id="00000000000001" author="diego.aires">
        <createTable tableName="tipoendereco">
            <column name="codtipoendereco" type="smallint"/>
            <column name="desctipoendereco" type="varchar(255)"/>
        </createTable>
        <insert tableName="tipoendereco">
            <column name="codtipoendereco" value="1"/>
            <column name="desctipoendereco" value="RESIDENCIAL"/>
        </insert>
        <insert tableName="tipoendereco">
            <column name="codtipoendereco" value="2"/>
            <column name="desctipoendereco" value="COMERCIAL"/>
        </insert>
        <insert tableName="tipoendereco">
            <column name="codtipoendereco" value="3"/>
            <column name="desctipoendereco" value="OUTROS"/>
        </insert>
        <insert tableName="tipoendereco">
            <column name="codtipoendereco" value="4"/>
            <column name="desctipoendereco" value="CAIXA POSTAL"/>
        </insert>
    </changeSet>
</databaseChangeLog>